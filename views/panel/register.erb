<div class="modal fade" id="myModalR" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">Registrar un empleado</h4>
      </div>
      <form action='/users/create' method="post">
        <div class="modal-body">
          <label>Ingresa tu nombre</label></br>
          <input type="text" id="name" name="name"/>  </br>
           <label>Ingresa tu c√≥digo de empleado</label></br>
          <input type="text" id="token_user" name="token_user" value="<%= SecureRandom.hex(2) %>"/>  </br>
           <label>Ingresa el token de administrador</label></br>
          <input type="text" id="pass" name="pass"/>  </br>
        </div>
        <div class="modal-footer">
          <input type="submit"  class="btn btn-default"></input>
        </div>
      </form>
    </div>
  </div>
</div>